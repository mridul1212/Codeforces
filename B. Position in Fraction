#include<bits/stdc++.h>

using namespace std;

int a,b,c;

int main()
{
   cin>>a>>b>>c;
   a%=b;
    //cout<<a<<endl;
    for (int i=1; i<=b; i++)
        {
            a=a*10;
          //  cout<<a<<endl;
            if (a/b==c)
                {
                    cout<<i<<endl;;
                    return 0;
                }
            a%=b;
             //cout<<a<<endl;

        }
   cout<<"-1"<<endl;
    return 0;
}
