#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int ar[n+1]={0};
        for(int i=1;i<=n;i++)
        {
            int c;
            cin>>c;
            ar[c]++;

        }
        int mx=0;
        for(int i=1;i<=n;i++)
        {
            mx=max(ar[i],mx);
        }
        cout<<mx<<endl;
    }
    return 0;
}
