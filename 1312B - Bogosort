#include<bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
      int a;
      cin>>a;
      int ar[a];
      for(int i=0;i<a;i++)
        cin>>ar[i];
      sort(ar,ar+a);
      for(int i=a-1;i>=0;i--)
        cout<<ar[i]<<" ";
      cout<<endl;

    }
    return 0;

}

