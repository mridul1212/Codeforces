#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
#define yo cout<<"YES"<<endl
#define yn cout<<"NO"<<endl
#define pii acos(-1.0)
#define sp cout<<" "
inline void filee()
{
	freopen("input.txt","r",stdin);
	freopen("output.txt","w", stdout);

}

int main()
{
	N a;
	cc>>a;
	while(a--)
	{
		N n;
		cc>>n;
		if(360%(180-n)==0)
			yo;
		else
			yn;
	}
	bai_ra 0;
}

