#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define bari_ja return
int main()
{
    int a,b,c,d,m1,m2;
    cc>>a>>b>>c>>d;
    m1=max((3*a/10),(a-(a*c)/250));
    m2=max((3*b/10),(b-(b*d)/250));
    if(m1>m2)
        co<<"Misha"<<endl;
    else if(m1<m2)
        co<<"Vasya"<<endl;
    else
        co<<"Tie"<<endl;
    bari_ja 0;
}
