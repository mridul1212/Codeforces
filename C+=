#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a,b,n;
        cin>>a>>b>>n;
        int an=0;
        while(max(a,b)<=n)
        {
            if(a>b)
                b+=a;
            else
                a+=b;
            an++;

        }
        cout<<an<<endl;

    }
    return 0;

}
