#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define bai_ra return
int main()
{
	int a=0,b=0;

	string s;
	cc>>s;
	for(int i=0;i<s.size();i++)
	{
		if(isupper(s[i]))
			a++;
		else
			b++;
	}
	if(a>b)
	{
char c;
		for(int i=0;i<s.size();i++)
		{
				c=toupper(s[i]);
              co<<c;
		}

	}
	else{
char c;
		for(int i=0;i<s.size();i++)
			{
				c=tolower(s[i]);
             co<<c;
			}

	}


	bai_ra 0;
}

