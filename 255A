#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,t, a1=0,a2=0,a3=0;
    cin>>a;
    for(int i=1; i<=a; i++)
    {
        cin>>t;
        if(i%3==2)
            a3+=t;

        else if(i%3==1)
            a2+=t;
        else
            a1+=t;
    }
    if(a1>a2&&a1>a3)
 cout << "back" << endl;


    else if(a2>a3&&a2>a1)
        cout << "chest" << endl;


    else
         cout << "biceps" << endl;
    return 0;

}

