#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a;
        cin>>a;
        int ar[a+1];
        for(int i=1;i<=a;i++)
            cin>>ar[i];
        int n=0;
        for(int i=1;i<=a;i++)
        {
            n=ar[i];
            int c=1;
            while(n!=i)
            {
                c++;
                n=ar[n];
            }
            cout<<c<<" ";

        }
        cout<<endl;
    }
    return 0;

}
