#include<bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--)
    {
        long long int a;
        cin>>a;
        int res=0;
        for(int i=1;i<=9;i++)
        {
            long long int p=0;
            while(p<=a)
            {
                p=p*10+i;
                if(p<=a)
                    res++;
            }
        }
        cout<<res<<endl;
    }
	return 0;
}


