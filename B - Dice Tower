#include<bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--)
    {
        long long int a;
        cin>>a;
        if(a<=14)
            cout<<"NO"<<endl;
        else if(1<=a%14&&a%14<7)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;

    }
	return 0;
}


