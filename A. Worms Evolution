#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0; i<n; i++)
        cin>>a[i];
    for(int i=0; i<n; i++)
    {
        for(int j=i+1; j<n; j++)
        {
            for(int k=0; k<n; k++)
            {
                if((k!=i&&k!=j)&&(a[i]+a[j]==a[k]))
            {
                cout<<k+1<<" "<<j+1<<" "<<i+1<<endl;
               return 0;
            }
        }
    }
}
cout<<-1<<endl;
return 0;
}
