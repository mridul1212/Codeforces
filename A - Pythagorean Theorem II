#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
#define yo cout<<"YES"<<endl
#define yn cout<<"NO"<<endl
#define pii acos(-1.0)
#define sp cout<<" "
inline void filee()
{
	freopen("input.txt","r",stdin);
	freopen("output.txt","w", stdout);
}
int main()
{
	N a,ans=0;
	cc>>a;
	for(N i=1;i<=a;i++)
	{
		for(N j=i;j<=a;j++)
		{
			N c=sqrt(i*i+j*j);
			if(c*c==i*i+j*j&&c<=a)
				ans++;
		}
	}
	co<<ans<<EE;

	bai_ra 0;
}

