#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
int main()
{
	N a,b=0,s[7];
	cc>>a;
	for(N i=0;i<7;i++)
		cc>>s[i];
	N c=s[0];
	while(c<a)
	{
		b++;
		b%=7;
		c+=s[b];
	}
	co<<b+1<<EE;
	bai_ra 0;
}

