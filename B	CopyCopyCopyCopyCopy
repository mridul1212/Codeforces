#include<bits/stdc++.h>
using namespace std;
int main ()
{
    int t,tem,n;
    set<int>s;
    cin>>t;
    while(t--)
    {
        cin>>n;
        for ( int i=0;i<n;i++)
        {
            cin>>tem;
            s.insert(tem);
        }
        cout<<s.size()<<endl;
        s.clear();

    }

}

