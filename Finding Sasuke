#include<bits/stdc++.h>
using namespace std;
#define INF 1234567890
#define ll long long

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a;
        cin>>a;
        int ar[a];
        for(int i=0;i<a;i++) cin>>ar[i];
        for(int i=0;i<a;i+=2)
            cout<<ar[i+1]<<" "<<-ar[i]<<" ";
        cout<<endl;
    }

	return 0;
}
