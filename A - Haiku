#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
int main()
{
	SS a,b,c;
	N a1=0,a2=0,a3=0;
	getline(cin,a);
	getline(cin,b);
	getline(cin,c);
	for(int i=0;i<a.size();i++)
	{
		if(a[i] == 'a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
			a1++;
	}
	for(int i=0;i<b.size();i++)
	{
		if(b[i] == 'a'||b[i]=='e'||b[i]=='i'||b[i]=='o'||b[i]=='u')
			a2++;
	}
	for(int i=0;i<c.size();i++)
	{
		if(c[i] == 'a'||c[i]=='e'||c[i]=='i'||c[i]=='o'||c[i]=='u')
			a3++;
	}
	if(a1==5&&a2==7&&a3==5)
		co<<"YES"<<EE;
	else
		co<<"NO"<<EE;
	bai_ra 0;
}

