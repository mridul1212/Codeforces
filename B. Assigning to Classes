#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a;
        cin>>a;
        a=a*2;
        vector<long long>ar(a);
        for(int i=0;i<a;i++)
        {
            cin>>ar[i];
        }
        sort(ar.begin(),ar.end());
        cout<<ar[a/2]-ar[a/2-1]<<endl;
    }
    return 0;
}
