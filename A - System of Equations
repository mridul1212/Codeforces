#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
int main()
{
	N a,b,c=0;
	cc>>a>>b;
	for(N i=0;i*i<=a&&i<=b;i++)
	{
		N j=a-i*i;
		if(i+j*j==b)
		{
			c+=1;
		}
	}
	co<<c<<EE;
	bai_ra 0;
}

