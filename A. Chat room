#include <iostream>
using namespace std;
int main()
{
    string str;
    cin>>str;
    int c=0;
    for ( int i=0; i<str.size();i++)
    {
        if ( str[i]=='h' && c==0)
        {
            c++;
        }
        else if  ( str[i]=='e' && c==1)
        {
            c++;
        }
        else if ( str[i]=='l' && c==2)
        {
            c++;
        }
        else if ( str[i]=='l'&& c==3)
        {
            c++;
        }
        else if ( str[i]=='o' && c==4)
        {
            c++;
        }
    }
    if ( c==5)
        cout<<"YES\n";
    else
        cout<<"NO\n";

    return 0;
}
