#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d,t1=0,t2=0,t3=0;
    cin>>a;
    while(a--)
    {
        cin>>b>>c>>d;

            t1+=b;
            t2+=c;
            t3+=d;
    }
    if(t1==0&&t2==0&&t3==0)
    {
        cout<<"YES"<<endl;
    }
    else
        cout<<"NO"<<endl;
    return 0;
}
