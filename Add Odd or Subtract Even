#include<iostream>
#include<math.h>
using namespace std;
int main ()
{
    long long  a, b,ans,tem;
    int t;
    cin>>t;
    while(t--)
    {
        cin>>a>>b;
        tem=(b-a);
        if (tem==0)
            cout<<0<<endl;
        else if (tem>0)
        {
            if (tem%2==0)
                cout<<2<<endl;
            else
                cout<<1<<endl;
        }
        else
        { if (tem%2==0)
                cout<<1<<endl;
            else
                cout<<2<<endl;
 
        }
 
 
 
    }
 
 
    return 0;
}
 
