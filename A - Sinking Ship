#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define bai_ra return
int main()
{
    int a;
    string s1,s2;
    vector<string>r,w,f,c;
    cc>>a;
    while(a--)
    {
        cc>>s1>>s2;
        if(s2=="captain")
            c.push_back(s1);
        else if(s2=="rat")
            r.push_back(s1);
        else if(s2=="woman"||s2=="child")
            w.push_back(s1);
        else if(s2=="man")
            f.push_back(s1);

    }
    for(int i=0; i<r.size(); i++)
        co<<r[i]<<endl;
    for(int i=0; i<w.size(); i++)
        co<<w[i]<<endl;
    for(int i=0; i<f.size(); i++)
        co<<f[i]<<endl;
    for(int i=0; i<c.size(); i++)
        co<<c[i]<<endl;
    bai_ra 0;
}

