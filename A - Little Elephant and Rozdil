#include<bits/stdc++.h>
using namespace std;
#define cc cin
#define co cout
#define lo long long int
#define N int
#define SS string
#define bai_ra return
#define EE endl
int main()
{
	lo n,b,c=1000000001,d=0,f=1;
	cc>>n;
	for(N i=1;i<=n;i++)
	{
		cc>>b;
		if(b<c)
		{
			c=b;
			d=i;
			f=1;
		}
		else if(b==c)
		{
			f+=1;
		}
	}
	if(f==1)
	{
		co<<d<<EE;
	}
	else
	{
		co<<"Still Rozdil"<<EE;
	}
	bai_ra 0;
}
