#include<bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
      int a;
      cin>>a;
      if(a==1)
        cout<<-1;
      else
      {
          cout<<2;
          for(int i=0;i<a-1;i++)
            cout<<3;
      }
      cout<<endl;


    }
    return 0;

}

