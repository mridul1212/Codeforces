#include<iostream>
#include<string>
using namespace std;

int main()
{
    int a,n;
    cin>>a;
    while(a--)
    {
        string str;
        cin>>str;
        n=str.size();
        n=n-1;
        if(str[n]=='o')
            cout<<"FILIPINO"<<endl;
        if(str[n]=='u')
            cout<<"JAPANESE"<<endl;
        if(str[n]=='a')
            cout<<"KOREAN"<<endl;

    }
    return 0;
}
