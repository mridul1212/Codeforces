#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,sum=0;
    cin>>t;
    int a[t],b[t];
    for(int i=0;i<t;i++)
        cin>>a[i];
    for(int i=0;i<t;i++)
        cin>>b[i];

    for(int i=0;i<t;i++)

               sum+=a[i]*b[i];
    if(sum==0)
            cout<<"Yes"<<endl;
    else
        cout<<"No"<<endl;
    return 0;

}
